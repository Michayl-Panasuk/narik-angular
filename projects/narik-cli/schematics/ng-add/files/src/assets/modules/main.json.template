{
  "imports": {
    "$$dataInfo": "./base/data-info.json",
    "$$toolbar": "./base/tool-bars.json",
    "$$uiDefaults": "./base/ui-default-options.json"
  },
  "metaData": {
    "translateItems": ["main", "toolbar", "<%= ui %>"],
    "dataInfo": "$$dataInfo.dataInfo",
    "uiDefaultOptions": [
      "$$uiDefaults.select",
      "$$uiDefaults.data-table",
      "$$uiDefaults.data-table-select",
      "$$uiDefaults.button",
      "$$uiDefaults.toolbar"
    ],
    "entities": [
      {
        "key": "branch",
        "fields": [
          {
            "name": "address",
            "required": false,
            "fieldType": "text",
            "showInList": false
          },
          {
            "name": "city",
            "required": false,
            "fieldType": "text",
            "order": 3
          },
          {
            "name": "name",
            "required": true,
            "fieldType": "text",
            "order": 1
          },
          {
            "name": "state",
            "required": false,
            "fieldType": "text",
            "order": 2
          },
          {
            "name": "zipCode",
            "required": false,
            "fieldType": "text",
            "order": 4
          }
        ],
        "defaultEntity": {}
      }
    ],
    "views": [
      {
        "key": "branch",
        "viewType": "Detail",
        "entity": "branch"
      },
      {
        "key": "branch-list",
        "viewType": "List",
        "entity": "branch"
      }
    ],
    <% if(layoutStr==="ngxadmin") { %>
    "menuItems": [
      {
        "title": "branch",
        "icon": "menu-outline",
        "children": [
          {
            "title": "branch_new",
            "link": "/branch"
          },
          {
            "title": "list_branch",
            "link": "/branch-list"
          }
        ]
      }
    ]
    <% } %>
    <% if(layoutStr==="coreui") { %>
    "menuItems": [
      {
        "title": "branch",
        "icon": "fa fa-bars",
        "children": [
          {
            "title": "branch_new",
            "link": "/branch",
            "icon": "fa fa-file"
          },
          {
            "title": "list_branch",
            "link": "/branch-list",
            "icon": "fa fa-list"
          }
        ]
      }
    ]
    <% } %>
    <% if(layoutStr==="architectui") { %>
    "menuItems": [
      {
        "title": "branch",
        "icon": "fa fa-bars",
        "children": [
          {
            "title": "branch_new",
            "link": "/branch",
            "icon": "fa fa-file"
          },
          {
            "title": "list_branch",
            "link": "/branch-list",
            "icon": "fa fa-list"
          }
        ]
      }
    ]
    <% } %>

    ,"toolbars": ["$$toolbar.detailToolBar", "$$toolbar.listToolBar"]
  }
}
