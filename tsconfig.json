{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@narik/core": [
        "dist/narik-core"
      ],
      "@narik/core/*": [
        "dist/narik-core/*"
      ],
      "@narik/common": [
        "dist/narik-common"
      ],
      "@narik/common/*": [
        "dist/narik-common/*"
      ],
      "@narik/app-core": [
        "dist/narik-app-core"
      ],
      "@narik/app-core/*": [
        "dist/narik-app-core/*"
      ],
      "@narik/infrastructure": [
        "dist/narik-infrastructure"
      ],
      "@narik/infrastructure/*": [
        "dist/narik-infrastructure/*"
      ],
      "@narik/ui-core": [
        "dist/narik-ui-core"
      ],
      "@narik/ui-core/*": [
        "dist/narik-ui-core/*"
      ],
      "@narik/ui-material": [
        "dist/narik-ui-material"
      ],
      "@narik/ui-material/*": [
        "dist/narik-ui-material/*"
      ],
      "@narik/ui-primeng": [
        "dist/narik-ui-primeng"
      ],
      "@narik/ui-primeng/*": [
        "dist/narik-ui-primeng/*"
      ],
      "@narik/ui-ng-bootstrap": [
        "dist/narik-ui-ng-bootstrap"
      ],
      "@narik/ui-ng-bootstrap/*": [
        "dist/narik-ui-ng-bootstrap/*"
      ],
      "@narik/ui-devextreme": [
        "dist/narik-ui-devextreme"
      ],
      "@narik/ui-devextreme/*": [
        "dist/narik-ui-devextreme/*"
      ],
      "@narik/jwt-authentication": [
        "dist/narik-jwt-authentication"
      ],
      "@narik/jwt-authentication/*": [
        "dist/narik-jwt-authentication/*"
      ],
      "@narik/client-storage": [
        "dist/narik-client-storage"
      ],
      "@narik/client-storage/*": [
        "dist/narik-client-storage/*"
      ],
      "@narik/signalr-client": [
        "dist/narik-signalr-client"
      ],
      "@narik/signalr-client/*": [
        "dist/narik-signalr-client/*"
      ],
      "@narik/ui-ngx-bootstrap": [
        "dist/narik-ui-ngx-bootstrap"
      ],
      "@narik/ui-ngx-bootstrap/*": [
        "dist/narik-ui-ngx-bootstrap/*"
      ],
      "@narik/ui-swimlane": [
        "dist/narik-ui-swimlane"
      ],
      "@narik/ui-swimlane/*": [
        "dist/narik-ui-swimlane/*"
      ],
      "@narik/ui-nebular": [
        "dist/narik-ui-nebular"
      ],
      "@narik/ui-nebular/*": [
        "dist/narik-ui-nebular/*"
      ]
    }
  }
}